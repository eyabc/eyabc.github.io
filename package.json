    "@docusaurus/theme-live-codeblock": "^2.0.0-alpha.73",
    "@mdx-js/react": "^1.6.21",
    "@primer/octicons-react": "^15.2.0",
    "clsx": "^1.1.1",
    "html-react-parser": "^1.2.6",
    "react": "^16.8.4",
    "react-codepen": "^0.1.0",
    "react-codepen-embed": "^1.0.2",
    "react-codepen-prefill-embed": "^0.0.2",
